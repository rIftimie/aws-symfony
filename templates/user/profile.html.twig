{% extends 'base.html.twig' %}

{% block title %}{{app.user}} Profile{% endblock %}

{% block body %}
     {% if app.user %}
        <h1>PERFIL de {{app.user}}</h1>

        <h2>Pokemons:</h2>
        {% for pokemon in app.user.pokemons %}
            <div>{{pokemon.name}}</div>
        {% endfor %}

    {% endif %}
{% endblock %}
